@model IEnumerable<SistemaPerfiladoCriminal.Models.InferenciaVictima>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Inferencias de víctimas</h2>

<p>
    @Html.ActionLink("Crear inferencia", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Descripción
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LstrTitulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LstrDescripcion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.LintId }) |
                @Html.ActionLink("Details", "Details", new { id = item.LintId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.LintId })
            </td>
        </tr>
    }

</table>
